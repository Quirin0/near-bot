exports.rg = (p,pushname) => {
	return`
	╭◪ *「 📝REGISTRO📝  」*
	╰───────────────╮
	╭───────────────╯
	├❏ Para se registrar :
	├❏「📌」Basta digitar ${p}rg nome/idade
	├❏ *exemplo : ${p}rg NOME/15*
	╰───────────────╯`
}
exports.rgerrado = (p,pushname) => {
